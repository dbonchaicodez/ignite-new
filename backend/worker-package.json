{
  "name": "ignite-cloudflare-worker",
  "version": "1.0.0",
  "description": "Cloudflare Worker for Ignite PDF platform R2 storage",
  "main": "cloudflare-worker.js",
  "scripts": {
    "deploy": "wrangler deploy",
    "deploy:prod": "wrangler deploy --env production",
    "deploy:dev": "wrangler deploy --env development",
    "dev": "wrangler dev",
    "logs": "wrangler tail",
    "logs:prod": "wrangler tail --env production"
  },
  "devDependencies": {
    "wrangler": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
